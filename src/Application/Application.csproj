  <ItemGroup>
    <Compile Update="Resources\Messages.en-US.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Messages.en-US.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Messages.pl-PL.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Messages.pl-PL.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Properties.en-US.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Properties.en-US.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Properties.pl-PL.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
